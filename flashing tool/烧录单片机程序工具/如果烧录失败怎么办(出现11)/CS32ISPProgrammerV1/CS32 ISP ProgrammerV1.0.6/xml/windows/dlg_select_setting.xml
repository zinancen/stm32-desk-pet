<?xml version="1.0" encoding="utf-8"?>
<dlg name="dlg_select_com" title="[Setting]" width="480" height="440" appwin="1" resize="0"
	translucent="255" frame="" crbk="000000"
	img-shadow="skins\default\winback_shadow.png" shadow-wlt="36" shadow-hlt="34" shadow-wrb="36" shadow-hrb="35">
	
	<base>
		<imgbtn name="button.close" pos="-45,0,-0,29" skin="IDB_BT_CLOSE" show="1"/>
		<text name="title" crtext="FFFFFF" crmark="800000" font="big"
				pos="10,4,200,25" title="[Setting]" response="0" show="1" />
	</base>
    
	<body>
		<area pos="0,27,-0,28" color="FFFFFF"  begin-transparent="70" end-transparent="70" />
		<tabctrl name="tabctrl.main"
				img-sep="skin:IDB_TAB_SEPERATOR--" img-hover="skin:IDB_TAB_HOVER--"
				font="bold" item-width="0" tab-height="0" tab-left-pading="0"
				pos="2,28,-1,-40"> 
			<tab name="tab.setting2" title="[CodeOption]" pos="0,2,-0,-0" show="1" >		
				<area pos="0,0,-0,-1" color="FFFFFF"  begin-transparent="0" end-transparent="0"/> 
				<nativewnd name="optionlist" pos="0,1,-1,-1" show="0" />
			</tab>
		</tabctrl>	
		
		<area pos="1,-40,-1,-1" color="FFFFFF"  begin-transparent="60" end-transparent="80"/>
		<button name="buttonok" skin="IDB_BT_DEFAULT" title="[Burnoption]"	pos="-190,-36,-110,-6" showfocus="0" show="1" />
		<button name="buttoncancel" skin="IDB_BT_DEFAULT" title="[Save]"	pos="-100,-36,-20,-6" showfocus="0" show="1" />	
	</body>
  
</dlg>